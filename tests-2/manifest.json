{
  "manifest_version": 3,
  "name": "Browser Lock",
  "version": "1.0",
  "action": {
    "default_popup": "password_authentication.html",
    "default_icon": {
      "16": "lock.png",
      "48": "lock.png",
      "128": "lock.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["tabs"]
}
